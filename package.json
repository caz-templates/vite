{
  "name": "@caz-templates/vite",
  "version": "0.1.0",
  "private": true,
  "description": "A template for creating vite-powered apps.",
  "keywords": [
    "vite",
    "caz-templates",
    "caz",
    "zce"
  ],
  "repository": "caz-templates/vite",
  "license": "MIT",
  "author": "zce <w@zce.me> (https://zce.me)",
  "main": "index.js",
  "scripts": {
    "test": "standard --fix && jest"
  },
  "jest": {
    "testTimeout": 30000,
    "modulePathIgnorePatterns": [
      "<rootDir>/template"
    ]
  },
  "renovate": {
    "extends": [
      "zce"
    ],
    "ignorePaths": [
      "template"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "template"
    ]
  },
  "devDependencies": {
    "caz": "0.1.3",
    "jest": "26.4.2",
    "standard": "15.0.1"
  }
}
