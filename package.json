{
  "name": "@caz-templates/vite",
  "version": "0.1.0",
  "private": true,
  "description": "A template for creating vite-powered apps.",
  "keywords": [
    "vite",
    "caz-templates",
    "caz",
    "zce"
  ],
  "repository": "caz-templates/vite",
  "license": "MIT",
  "author": "zce <w@zce.me> (https://zce.me)",
  "main": "index.js",
  "scripts": {
    "test": "standard --fix && jest"
  },
  "jest": {
    "testTimeout": 30000,
    "modulePathIgnorePatterns": [
      "<rootDir>/template"
    ]
  },
  "renovate": {
    "extends": [
      "zce"
    ],
    "ignorePaths": [
      "template"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "template"
    ]
  },
  "dependencies": {
    "vue": "^3.0.2",
    "vite": "^1.0.0-rc.9",
    "@vue/compiler-sfc": "^3.0.2"
  },
  "devDependencies": {
    "caz": "0.1.3",
    "jest": "26.6.3",
    "standard": "16.0.1"
  }
}
